{
  "name": "text",
  "simulation": {
    "type": "nested",
    "properties": [
      {
        "tradeables": [
          {
            "type": "nested",
            "properties": {
              "tradeableType":  "text",
              "numTradeables": "integer"
            }
          }
        ],
        "valuations": {
          "type": "nested",
          "properties": {
            "distribution": "text",
            "generator": "text",

          }
        },
        "endowments": {
          "type": "nested",
          "properties": {
            "tradeable": [
              {
                "type": "nested",
                "properties": {
                  "tradeableType": "text" ,
                  "numTradeables": "integer"
                }
              }
            ],
            "money": "float"
          }
        },
        "seqmarket": [
          {
            "type": "nested",
            "properties": {
              "simmarket": [
                {
                  "type": "nested",
                  "properties": {
                    "marketrules": {
                      "type": "nested",
                      "properties": {
                        "allocationRule": "text",
                        "paymentRule": "text" ,
                        "queryRule": "text",
                        "activityRule": "text",
                        "informationRevelationPolicy": "text",
                        "terminationCondition": "text"
                      }
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    ],
    "startingDelay": "integer"
  }
}


